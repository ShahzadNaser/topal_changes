 "html": "<table width=\"100%\">\n    <thead>\n    <tr>\n        <td width=\"100%\">\n        </td>\n    </tr>\n    </thead>\n    <tbody>\n    <tr>\n        <td>\n            <div class=\"content\">\n                <div class=\"col-xs-12\">\n                    <div class=\"row section-break\">\n                        <div class=\"col-xs-8\">\n                            <table class=\"table table-bordered text-center\">\n                                <tr>\n                                    <td>TAPAL TEX PVT LTD</td>\n                                </tr>\n                                <tr>\n                                    <td>Spunlac Non-Woven Fabric</td>\n                                </tr>\n                                <tr>\n                                    <td>Order from Invoice Month-(March 2023)</td>\n                                </tr>\n                            </table>\n                        </div>\n                         <div class=\"col-xs-4\">\n                            <img src=\"/files/logo.png\" style=\"width: 100%!important;\">\n                         </div>\n                        <div class=\"col-xs-12\">\n                            <div class=\"invoice-top-div\">\n                                <div class=\"row\">\n                                    <div class=\"col-md-12\">\n                                        <table class=\"table table-bordered\">\n                                            <tr>\n                                                <td width=\"20%\">\n                                                   To :\n                                                </td>\n                                                <td  width=\"20%\">\n                                                    {{doc.customer_name or \"\"}}\n                                                </td>\n                                                <td width=\"20%\">\n                                                    From:\n    